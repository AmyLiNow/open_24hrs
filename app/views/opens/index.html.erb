<p id="notice"><%= notice %></p>

<h1>Listing Opens</h1>

<table>
  <thead>
    <tr>
      <th>Yelp</th>
      <th>Name</th>
      <th>Street</th>
      <th>City</th>
      <th>State</th>
      <th>Zip</th>
      <th>Phone</th>
      <th>Neighborhood</th>
      <th>Website</th>
      <th>Category</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @opens.each do |open| %>
      <tr>
        <td><%= open.yelp %></td>
        <td><%= open.name %></td>
        <td><%= open.street %></td>
        <td><%= open.city %></td>
        <td><%= open.state %></td>
        <td><%= open.zip %></td>
        <td><%= open.phone %></td>
        <td><%= open.neighborhood %></td>
        <td><%= open.website %></td>
        <td><%= open.category %></td>
        <td><%= open.latitude %></td>
        <td><%= open.longitude %></td>
        <td><%= link_to 'Show', open %></td>
        <td><%= link_to 'Edit', edit_open_path(open) %></td>
        <td><%= link_to 'Destroy', open, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Open', new_open_path %>
